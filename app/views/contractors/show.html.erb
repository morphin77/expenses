<div class="page-header">
  <div class="container-fluid">
    <div class="btn-group pull-right">
      <button type="button" class="btn btn-primary dropdown-toggle" data-toggle="dropdown">
        Действия
        <span class="caret"></span>
      </button>
      <ul class="dropdown-menu">
        <li><%= link_to 'Настройки', edit_contractor_path(@contractor) %></li>
      </ul>
    </div>
    <h1>
      <%= @contractor.title %>
      <small><%= @contractor.address %></small>
    </h1>
  </div>
</div>
<table class="table table-hover">
  <tr>
    <th>Контактное лицо</th>
    <th>Должность</th>
    <th>Адрес эл. почты</th>
    <th>Номер телефона</th>
    <th>Комментарий</th>
    <th></th>
  </tr>
  <% for item in @contractor.contractor_items  %>
  <tr>
    <td><%= item.contact_person_name %></td>
    <td><%= item.position %></td>
    <td><%= item.email %></td>
    <td><%= item.phone %></td>
    <td><%= item.comment %></td>
    <td><%= link_to 'Destroy', item, method: :delete, data: { confirm: 'Вы Уверены что хотите удалить этот объект?' } %></td>
  </tr>
  <% end %>
</table
